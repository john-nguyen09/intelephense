    "subleveldown": "^4.0.0",
    "php7parser": "file://../php7parser"
  },
  "devDependencies": {
    "memdown": "^4.0.0",
    "@types/async-lock": "^1.1.1",
    "@types/chai": "^4.1.7",
    "@types/fs-extra": "^7.0.0",
    "@types/level-codec": "^9.0.0",
    "@types/leveldown": "^4.0.0",
    "@types/levelup": "^3.1.1",
    "@types/memdown": "^3.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  }
