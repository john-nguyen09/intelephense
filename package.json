    "signature",
    "diagnostics",
    "format",
    "autocomplete",
    "symbols"
  ],
  "bugs": {
    "url": "https://github.com/john-nguyen09/intelephense/issues"
  },
  "homepage": "http://intelephense.net/",
  "repository": {
    "type": "git",
    "url": "https://github.com/john-nguyen09/intelephense.git"
  },
  "dependencies": {
    "async-lock": "^1.2.0",
    "fuzzysearch": "^1.0.3",
    "graceful-fs": "^4.1.15",
